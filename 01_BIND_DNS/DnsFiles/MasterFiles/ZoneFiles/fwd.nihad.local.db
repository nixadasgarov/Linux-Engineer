@       IN SOA  @ nihad.local. (
                                        0       ; serial
                                        1D      ; refresh
                                        1H      ; retry
                                        1W      ; expire
                                        3H )    ; minimum

        NS      @
        A       127.0.0.1
        AAAA    ::1


master IN A 192.168.1.11
slave  IN A 192.168.1.12
test   IN A 192.168.1.13
cname  IN CNAME test.nihad.local.
