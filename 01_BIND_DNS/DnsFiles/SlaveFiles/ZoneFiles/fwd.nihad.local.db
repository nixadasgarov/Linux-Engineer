$ORIGIN .
$TTL 10800	; 3 hours
nihad.local		IN SOA	nihad.local. nihad.local. (
				0          ; serial
				86400      ; refresh (1 day)
				3600       ; retry (1 hour)
				604800     ; expire (1 week)
				10800      ; minimum (3 hours)
				)
			NS	nihad.local.
			A	127.0.0.1
			AAAA	::1
$ORIGIN nihad.local.
cname			CNAME	test
master			A	192.168.1.11
slave			A	192.168.1.12
test			A	192.168.1.13
